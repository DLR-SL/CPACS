<?xml version="1.0" encoding="UTF-8"?>
<cpacs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
       xsi:noNamespaceSchemaLocation="../schema/cpacs_schema.xsd" 
	   xsi:schemaLocation="http://www.cpacs.de/cpacsTechnologyAssumptions ../extensions/cpacsTechnologyAssumptions.xsd">
	<header>
		<name>CPACS Extensions example file</name>
		<version>1.0.0</version>
		<versionInfos>
			<versionInfo version="1.0.0">
				<creator>DLR-SL</creator>
				<timestamp>2023-04-27T12:37:00</timestamp>
				<description>Demonstration on how to use the extensions-node in CPACS.</description>
				<cpacsVersion>3.4</cpacsVersion>
			</versionInfo>
		</versionInfos>
	</header>
	<vehicles>
		<aircraft>
			<model uID="myAircraft">
				<name>myAircraft</name>
			</model>
		</aircraft>
	</vehicles>
	<extensions>
		<extension>
			<technologyAssumptions xmlns="http://www.cpacs.de/cpacsTechnologyAssumptions" schemaVersion="1.0">
				<assumption>
					<name>Test assumption</name>
					<componentUID>myAircraft</componentUID>
					<value>1.</value>
					<whateverElse>is needed</whateverElse>
				</assumption>
			</technologyAssumptions>
		</extension>
	</extensions>
</cpacs>
